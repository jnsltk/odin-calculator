{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-alert": "off",
        "no-console": "off",
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "error",
            4
        ]
    }
}
